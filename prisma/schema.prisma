generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Link {
  id           Int      @id @default(autoincrement())
  code         String   @unique
  long_url     String
  click_count  Int      @default(0)
  created_at   DateTime @default(now())
  last_clicked DateTime?
}
